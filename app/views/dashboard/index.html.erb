<!-- app/views/dashboard/index.html.erb -->

<div class="d-flex justify-content-between align-items-center">
    <h2>Accounts
    <%= link_to image_tag('plus.svg', :size => "35x35"), new_account_path %>
    </h2>
</div>    

<div id="accounts">
  <% @accounts.each do |account| %>
    <a href="<%= account_path(account) %>" class="card-link">
      <div class="card border-white">
        <div class="card-body">
          <%= render account %>
        </div>
      </div>
    </a>
  <% end %>
</div>

